{
  "name": "gthubweb",
  "version": "0.1.0",
  "description": "New GTHub's website design using Node.js and Express",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "test_connection": "node ./database/test_connection.js",
    "test": "npm run lint && cross-env NODE_ENV=test mocha -R spec api/test --exit",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "create_database": "npm run create_tables && npm run init_data && npm run insert_testdata",
    "create_tables": "node ./database/create_tables.js",
    "init_data": "node ./database/init.js",
    "insert_testdata": "node ./database/insert_testdata.js"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@types/async": "^3.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/country-list": "^2.1.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/supertest": "^2.0.10",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.5",
    "country-code-emoji": "^1.0.0",
    "country-list": "^2.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-html": "^6.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "ical-generator": "^1.15.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.0",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "nuxt": "^2.14.7",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primevue": "^2.2.0",
    "sequelize": "^5.22.3",
    "supertest": "^4.0.2",
    "typescript": "^4.0.3",
    "vue": "^2.6.12",
    "vue-server-renderer": "^2.6.12",
    "vue2-timepicker": "^1.1.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@types/chai": "^4.2.14",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chokidar": "^3.4.3",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-vue": "^7.1.0",
    "mocha": "^8.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
